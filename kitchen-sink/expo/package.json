{
  "name": "@marceloterreiro/kitchen-sink-expo",
  "private": true,
  "main": "./index.js",
  "scripts": {
    "analyze:android": "source-map-explorer 'dist/_expo/static/js/android/*.js' 'dist/_expo/static/js/android/*.js.map'",
    "analyze:ios": "source-map-explorer 'dist/_expo/static/js/ios/*.js' 'dist/_expo/static/js/ios/*.js.map'",
    "android": "expo start --android",
    "build:android": "bunx expo export --source-maps --platform android",
    "build:ios": "bunx expo export --source-maps --platform ios",
    "clean": "rm -rf dist && rm -rf node_modules",
    "ios": "expo start --ios",
    "lint": "eslint src",
    "start": "expo start",
    "typecheck": "tsc --noEmit",
    "web": "expo start --web"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.2.4",
    "@marceloterreiro/flash-calendar": "*",
    "@shopify/flash-list": "^2.0.3",
    "expo": "53.0.22",
    "expo-status-bar": "~1.11.1",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@marceloterreiro/eslint-config": "*",
    "@marceloterreiro/tsconfig": "*",
    "@types/react": "~19.0.0",
    "eslint": "^8.56.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.3.3"
  },
  "version": null
}
